<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.example.appsdev.Home.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_tools"
            app:destination="@id/tools" />
        <action
            android:id="@+id/action_home_to_dogedex"
            app:destination="@id/dogedex" />
        <action
            android:id="@+id/action_home_to_marketFB"
            app:destination="@id/marketFB" />
    </fragment>
    <fragment
        android:id="@+id/dogedex"
        android:name="com.example.appsdev.Home.Dogedex.Dogedex"
        android:label="fragment_dogedex"
        tools:layout="@layout/fragment_dogedex" />
    <fragment
        android:id="@+id/tools"
        android:name="com.example.appsdev.Home.Tools.Tools"
        android:label="fragment_tools"
        tools:layout="@layout/fragment_tools" >
        <action
            android:id="@+id/action_tools_to_anyChart"
            app:destination="@id/anyChart" />
        <action
            android:id="@+id/action_tools_to_camara"
            app:destination="@id/camara" />
        <action
            android:id="@+id/action_tools_to_recyclerView"
            app:destination="@id/recyclerView" />
    </fragment>
    <fragment
        android:id="@+id/anyChart"
        android:name="com.example.appsdev.Home.Tools.AnyChart.AnyChart"
        android:label="fragment_any_chart"
        tools:layout="@layout/fragment_any_chart" />
    <fragment
        android:id="@+id/camara"
        android:name="com.example.appsdev.Home.Tools.Camara.Camara"
        android:label="fragment_camara"
        tools:layout="@layout/fragment_camara" />
    <fragment
        android:id="@+id/recyclerView"
        android:name="com.example.appsdev.Home.Tools.RecyclerView.RecyclerView"
        android:label="fragment_recycler_view"
        tools:layout="@layout/fragment_recycler_view" />
    <fragment
        android:id="@+id/marketFB"
        android:name="com.example.appsdev.Home.MarketFB.MarketFB"
        android:label="fragment_market_fb"
        tools:layout="@layout/fragment_market_fb" />
</navigation>